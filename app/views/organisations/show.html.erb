<div class="row">
  <div class="col-md-6">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h2 class="panel-title"><%= @organisation.name %></h2>
      </div>
      <div class="panel-body">
        <%= image_tag @organisation.logo, class: 'img-responsive img-thumbnail' %>
<p>
  <strong>Org type:</strong>
  <%= @organisation.org_type %>
</p>

<p>
  <strong>Info:</strong>
  <%= @organisation.info %>
</p>

<p>
  <strong>Contact name:</strong>
  <%= @organisation.contact_name %>
</p>

<p>
  <strong>Contact email:</strong>
  <%= @organisation.contact_email %>
</p>

<p>
  <strong>Contact phone:</strong>
  <%= @organisation.contact_phone %>
</p>

<p>
  <strong>Street:</strong>
  <%= @organisation.street %>
</p>

<p>
  <strong>Suburb:</strong>
  <%= @organisation.suburb %>
</p>

<p>
  <strong>State:</strong>
  <%= @organisation.state %>
</p>

<p>
  <strong>Postcode:</strong>
  <%= @organisation.postcode %>
</p>

<p>
  <strong>Latitude:</strong>
  <%= @organisation.latitude %>
</p>

<p>
  <strong>Longitude:</strong>
  <%= @organisation.longitude %>
</p>

<%= link_to 'Edit', edit_organisation_path(@organisation) %> |
<%= link_to 'Back', organisations_path %>
      </div>
    </div>
  </div>
</div>


